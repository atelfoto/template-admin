{
    "name": "atelfoto/template-admin",
    "description": "Admin plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "support": {
        "source": "https://github.com/atelfoto/template-admin"
    },
    "require": {
        "cakephp/cakephp": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Admin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Admin\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}